<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="set_route_inherit_res_config_settings" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="website_sale.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='digital_content_setting']" position="before">
                <div class="col-12 col-lg-6 o_setting_box">
                    <div class="o_setting_right_pane">
                        <div class="mt16">
                            <label for="dr_route" class="o_light_label col-lg-3"/>
                            <field name="dr_route" class="oe_inline"/>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>

